		<include>CommonBackground</include>
		<include condition="!IsEmpty(Window(Home).Property(HomeDefault))">HomeDefault</include>
		<include condition="!IsEmpty(Window(Home).Property(HomeTopMenu))">HomeTopMenu</include>
		<include condition="!IsEmpty(Window(Home).Property(HomeVerticalMenu))">HomeVerticalMenu</include>
	</controls>
</window>